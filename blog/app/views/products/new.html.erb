<%= form_for @product do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :catalogy%><br>
    <select name="product[catalogy]">
        <%= options_from_collection_for_select(Catalogy.all, :id, :text) %>
    </select>
  </p>
 
  <p>
    <%= f.submit %>
  </p>

  

  


<% end %>
